{
    "manifest_version": 3,
    "name": "Twitch_StreamOrganizer",
    "version": "1.1",
    "icons": { "128": "icon.png" },
    "permissions": ["storage"],
    "content_scripts": [
    {
        "matches": ["https://www.twitch.tv/*"],
        "css": ["styles.css"],
        "js": ["content-script.js"]
    }
    ],
    "host_permissions": ["https://www.twitch.tv/*"]
}